    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4"
  },
  "peerDependencies": {
    "react-scanner": ">=1.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
