{
  "name": "react-scanner-studio",
  "type": "module",
  "version": "1.0.0",
  "main": "dist/index.js",
  "keywords": [
    "react",
    "react-scanner",
    "react-components",
    "design-system",
    "design-system-observability",
    "usage",
    "analytics",
    "insights",
    "codebase-analysis",
    "dashboard"
  ],
  "bin": {
    "react-scanner-studio": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vimalmunjani/react-scanner-studio.git"
  },
  "homepage": "https://react-scanner-studio/",
  "logo": "logo.png",
  "author": "Vimal Munjani <vimal.j.munjani@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "build:ui": "vite build --config ui/vite.config.ts",
    "build:docs": "vitepress build docs",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:ui": "vite --config ui/vite.config.ts",
    "dev:docs": "vitepress dev docs",
    "preview:docs": "vitepress preview docs",
    "prepublishOnly": "yarn build",
    "publish:npm": "yarn build && npm publish",
    "prepare": "husky",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
